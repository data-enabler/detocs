{
  "compilerOptions": {
    "outDir": "build",
    "target": "ES2019",
    "module": "ES2015",
    "strict": true,
    "incremental": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@export/*": [ "export/*" ],
      "@import/*": [ "import/*" ],
      "@models/*": [ "models/*" ],
      "@server/*": [ "server/*" ],
      "@services/*": [ "services/*" ],
      "@upload/*": [ "upload/*" ],
      "@util/*": [ "util/*" ],
      "@web/*": [ "web/*" ],
      "package.json": [ "package.json" ]
    },
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "h"
  }
}
