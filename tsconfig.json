{
  "compilerOptions": {
    "target": "ES2019",
    "module": "ES2015",
    "strict": true,
    "incremental": true,
    "skipDefaultLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react",
    "jsxFactory": "h"
  },
  "include": [ "export", "import", "models", "server", "types", "upload", "util", "web/js" ],
  "exclude": [ "**/node_modules" ]
}
